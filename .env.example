# Environment Variables for Journal App Backend
# Copy this file to .env for local development

# MongoDB Configuration
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/database?retryWrites=true&w=majority&appName=appname
MONGODB_DATABASE=journalAppDB

# Server Configuration (Azure will set PORT automatically)
PORT=8080

# Application Configuration
SESSION_TIMEOUT=3600
SESSION_SECRET=your-super-secret-session-key-here

# Azure Specific (these will be set automatically by Azure)
# WEBSITES_PORT=8080
# WEBSITES_CONTAINER_START_TIME_LIMIT=600
